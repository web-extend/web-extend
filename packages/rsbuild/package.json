{
  "name": "@web-extend/rsbuild-plugin",
  "version": "0.0.1-beta.4",
  "description": "A Rsbuild plugin of WebExtend.",
  "keywords": ["web extensions", "browser extensions", "rsbuild plugin"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-extend/web-extend.git"
  },
  "homepage": "https://web-extend.github.io/web-extend/",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks && npm run build",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.49.1",
    "@rsbuild/core": "^1.2.3",
    "@rslib/core": "^0.1.5",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.12.0",
    "@types/webextension-polyfill": "^0.12.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@web-extend/manifest": "workspace:*",
    "typescript": "^5.7.3",
    "webextension-polyfill": "^0.12.0"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.1.6"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@babel/parser": "^7.26.7",
    "@babel/traverse": "^7.26.7",
    "@babel/types": "^7.26.7"
  }
}
